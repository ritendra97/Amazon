import java.util.ArrayList;
import java.util.Arrays;

public clas s Main4{

    public static void Answer(String str, int m){

        m = m%26;
        for(int i = i=0;i<str.length();i++){
            int c = str.charAt(i);
            c+=m;
            if(c>'z'){
                c = 96+(c%122);
            }
            System.out.print    ((char)c);
        }
    }

    public static void main(String [] args){
        String str = "middleoutz";
        Answer(str,2);
    }
}
